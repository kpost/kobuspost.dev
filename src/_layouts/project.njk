---
layout: base.njk
section: project
---
<div class="page-container">
  <h1 class="project-title">
    {% if emoji %}
      <span class="project-card__emoji">{{ emoji }}</span>
    {% elif image %}
      {% iconimage image, title, "project-icon" %}
    {% endif %}
    <span>{{title}}</span>
  </h1>
  <hr/>
  {{ layoutContent | safe }}
  <nav>
    <a href="{{ '/projects/' | url }}">‚Üê View All Projects</a>
  </nav>
</div>